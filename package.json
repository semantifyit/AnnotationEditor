{
  "name": "annotation-editor",
  "version": "1.0.0",
  "homepage": "https://github.com/semantifyit/annotation-editor",
  "repository": {
    "type": "git",
    "url": "github.com:semantifyit/annotation-editor"
  },
  "main": "server/app.js",
  "scripts": {
    "start": "npm run client-build && npm run server-prod",
    "dev": "concurrently -â€“kill-others-on-fail \"npm run client-dev\" \"npm run server-dev\"",
    "client-dev": "cd client && npm run start",
    "client-build": "cd client && npm run build",
    "server": "cd server && node app.js",
    "server-dev": "cd server && nodemon app.js",
    "server-prod": "cd server && cross-env NODE_ENV=production node app.js",
    "analyze": "source-map-explorer client/build/static/js/main.*",
    "analyze-deps": "find client/build/static/js/**.chunk.js | head -n 1 | xargs source-map-explorer",
    "validate:ts": "cd client && tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate:ts"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "tabWidth": 2
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/xml2js": "^0.4.4",
    "api-mapping": "file:api-mapping",
    "archiver": "^3.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.0.1",
    "config": "^3.0.0",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "jsonld": "^1.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "n3": "^1.0.1",
    "rdflib": "^0.19.1",
    "react": "^16.6.3",
    "react-ace": "^6.2.0",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.5.2",
    "react-onclickoutside": "^6.7.1",
    "react-popper": "^1.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "react-select": "^2.1.0",
    "react-toastify": "^4.4.0",
    "reactstrap": "^6.5.0",
    "split.js": "^1.5.9",
    "uuid": "latest"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "@types/react-onclickoutside": "^6.7.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-toastify": "^4.0.1",
    "@types/reactstrap": "^6.4.2",
    "@types/uuid": "^3.4.4",
    "husky": "^1.2.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.6",
    "prettier": "^1.14.3",
    "source-map-explorer": "^1.6.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.2"
  }
}
